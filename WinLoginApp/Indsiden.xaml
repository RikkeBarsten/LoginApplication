<Page x:Class="WinLoginApp.Indsiden"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WinLoginApp"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="300"
      Title="Logindsiden">
    <Canvas>
        <Label x:Name="labelLogin" Content="Log ind på denne side" Canvas.Left="10" Canvas.Top="10"/>
        <Label x:Name="labelUsername" Content="Brugernavn" Canvas.Left="10" Canvas.Top="84"/>
        <TextBox x:Name="textBoxUsername" Height="23" Canvas.Left="15" TextWrapping="Wrap" Text="" Canvas.Top="115" Width="246"/>
        <Label x:Name="labelKodeord" Content="Kodeord" Canvas.Left="10" Canvas.Top="175"/>
        <PasswordBox x:Name="passwordBox" Canvas.Left="15" Canvas.Top="201" Width="244" Height="21"/>
        <Button x:Name="buttonLogin" Content="Log ind" Canvas.Left="180" Canvas.Top="248" Width="75" Click="buttonLogin_Click"/>
        <Button x:Name="NyKonto" Content="Ny konto" Canvas.Left="189" Canvas.Top="40" Width="70" Click="NyKonto_Click"/>
        

    </Canvas>

</Page>
